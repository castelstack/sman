n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    uploadFile();\n\n    setGistState((prev) => ({ ...prev, image: imageUrl }));\n  };\n\n  return (\n    <Container>\n      <HeadBox>\n        <Heading>Write Your Stingy Gist</Heading>\n      </HeadBox>\n\n      <form onSubmit={handleSubmit}>\n        <WriteIn>\n          <TextTitle>Pick a stingy title</TextTitle>\n          <Select\n            name=\"title\"\n            value={gistState.title}\n            onChange={(e) =>\n              setGistState({\n                ...gistState,\n                title: e.target.value,\n              })\n            }\n          >\n            <Option value=\"All\">Choose stingy title</Option>\n            <Option value=\"Urgent2k\">Urgent2k</Option>\n            <Option value=\"Valentine\">Valentine</Option>\n            <Option value=\"Transport\">Transport</Option>\n          </Select>\n\n          <TextGist\n            label=\"Write your rules\"\n            name=\"description\"\n            placeholder=\"Your Stingy Gist\"\n            onChange={(e) =>\n              setGistState({\n                ...gistState,\n                description: e.target.value,\n              })\n            }\n            value={gistState.description}\n          />\n        </WriteIn>\n\n        <Label htmlFor=\"upload\">\n          <PhotoCamera />\n          Add picture\n        </Label>\n        <InputImg\n          type=\"file\"\n          accept=\"image/*\"\n          onChange={getInputFile}\n          id=\"upload\"\n        />\n        <img src={imageUrl} alt=\"upload\" />\n\n        <Post value=\"Post\" type=\"submit\" onSubmit={handleSubmit} />\n      </form>\n    </Container>\n  );\n};\n\nexport default WriteGist;\n",["221","222"],"/home/pappyj/Documents/SMAN-UI/src/containers/header/header.jsx",[],["223","224"],"/home/pappyj/Documents/SMAN-UI/src/containers/input-email/input-email.jsx",["225","226"],"import React from \"react\";\nimport { HeadText, MedText } from \